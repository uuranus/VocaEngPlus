<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vocaengplus.vocaengplus">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ui.VocaEngPlusApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VocaEngPlus">
        <activity
            android:name=".ui.login.RoutingActivity"
            android:exported="true"
            android:theme="@style/Theme.VocaEngPlus.Starting">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.profile.ProfileActivity" />
        <activity
            android:name=".ui.login.MainActivity"
            android:theme="@style/Theme.VocaEngPlus.NoActionBar" />
        <activity android:name=".ui.setting.AddCategory" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.login.RegisterActivity" />
        <activity android:name=".ui.community.CommunityActivity" />
        <activity android:name=".ui.statistics.StatisticsActivity" />
        <activity android:name=".ui.search.SearchActivity" />
        <activity android:name=".ui.myWord.MyWordActivity" />
        <activity android:name=".ui.review.ReviewActivity" />
        <activity android:name=".ui.test.TestActivity" />
        <activity android:name=".ui.wordList.WordActivity" />
        <activity android:name=".ui.setting.SettingActivity" />
    </application>

</manifest>